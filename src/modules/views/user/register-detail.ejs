  <script>
    $(function() {
        $('#file_modal').on('hidden.bs.modal', function (e) {
            let modal = $(e.target);
            if (modal.attr('dismissed') == 'false') {
                set_checked(modal.find('#decline_btn').get(0), false)
            }
        })
     });  
    function set_checked(el, accepted = true){
        let element = $(el);
        let modal = $('#file_modal');
        let id = '#'+modal.find('.el_id').val();
        let span = $(id).next().find('strong');
        span.show();
        if (accepted) {
            span.css('color', 'green');
            span.text('Accepted');
        } else {
            span.css('color', 'red');
            span.text('Declined');
        }
        $(id).prop('checked', accepted);
        modal.attr('dismissed', true)
        modal.modal('hide');
    }

    let checkBoxes = [{"key":"agrem","title":"Client Agreement","file":"assets\/pdf\/Customer Identification Program Notice.pdf"},{"key":"day","title":"Day Trading Risk Disclosure","file":"assets\/pdf\/Guardian Day Trading Risk Disclosure.pdf"},{"key":"penny","title":"Penny Stocks Disclosure","file":"assets\/pdf\/Penny Stock Risk Disclosure.pdf"},{"key":"anti_money","title":"Anti Money Laundering Disclosure","file":""},{"key":"online_risj","title":"Understanding Online Trading Risk Disclosure","file":""},{"key":"margin_disc","title":"Margin Disclosure","file":"assets\/pdf\/Guardian Trading Margin Risk.pdf"},{"key":"petren","title":"Pattern Day Trade Disclosure","file":""}];
    //let link = 'file:///C:/Users/pc/Downloads/Steps-Form%20Maker/';
     link = 'file:///../public/assets/pdf/';
    
    function showFileModal(el, index) {
        let element = $(el);
        let modal = $('#file_modal');
        modal.find('.modal-title').text(checkBoxes[index].title);
        modal.find('.el_id').val(checkBoxes[index].key);
        let file_path = link+checkBoxes[index].file;
        if (checkBoxes[index].file == '') {
            file_path = null;
        }
        modal.find('iframe').attr('src', file_path);
        modal.attr('dismissed', false)
        modal.modal('show');
    }
</script>

